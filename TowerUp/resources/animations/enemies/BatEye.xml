<animations>
    <animation key="BatEyeFly">
        <frame x="0" y="0" width="64" height="64" repeat-x="6"/>
    </animation>
    <animation key="BatEyeShoot">
        <frame x="0" y="64" width="64" height="64" repeat-x="6" repeat-y="2"/>
        <frame x="0" y="192" width="64" height="64"/>
    </animation>
    <animation key="BatEyeDeath">
        <frame x="64" y="192" width="64" height="64" repeat-x="5"/>
        <frame x="0" y="256" width="64" height="64" repeat-x="6"/>
    </animation>
</animations>