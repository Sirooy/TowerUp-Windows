<animations>
    <animation key="CoinDefault">
        <frame x="0" y="0" width="16" height="16" repeat-x="8"/>
    </animation>
    <animation key="CoinPickedUp">
        <frame x="0" y="16" width="16" height="16" repeat-x="8"/>
